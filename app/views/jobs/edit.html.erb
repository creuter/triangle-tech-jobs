<h1>Edit Job</h1>

<%= link_to "Cancel", job_path(@job) %>

<%= simple_form_for @job do |form| %>
  <%= form.input :title, label: "Job title" %>
  <%= form.input :company, label: "Company name" %>
  <%= form.input :description, as: :text %>
  <%= form.input :url, label: "Link to company's website" %>
  <%= form.button :submit %>
<% end %>
